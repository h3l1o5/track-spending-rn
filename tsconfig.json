{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "lib": ["es2015"],
    "jsx": "react",
    "strict": true,
    "alwaysStrict": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "strictFunctionTypes": true,
    "noImplicitAny": true,
    "noImplicitThis": true
  },
  "exclude": [
    "index.js",
    "android",
    "ios",
    "build",
    "node_modules",
    ".coverage",
    "native-assets",
    "jest"
  ],
  "types": ["jest"]
}
